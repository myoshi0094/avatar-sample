{
  "name": "avatar-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"frontend,backend\" --prefix-colors \"cyan,green\" \"pnpm --filter frontend dev\" \"go run ./backend/main.go\"",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "go run ./backend/main.go",
    "build": "pnpm --filter frontend build",
    "lint": "pnpm --filter frontend lint",
    "format": "pnpm --filter frontend format"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
